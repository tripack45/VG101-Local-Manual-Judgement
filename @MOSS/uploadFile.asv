function uploadFile(handle, file, id )
%   UPLOADFILE Summary of this function goes here
%   Detailed explanation goes here
endl;
        content = fileread(file);
        size    = length(content);
		file_name_fixed = str_replace(' ', '_', file);
		fwrite(handle, ['file ',num2str(id),' ' ...
            ,obj.options.l,' ', num2str(size),' ',file_name_fixed,'\n']);
		fwrite(handle, content );
end

